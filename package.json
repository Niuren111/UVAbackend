{
  "name": "drone-industry-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://niuren111.github.io/UAV/",
  "scripts": {
    "dev": "vite",
    "dev:admin": "vite --config vite.admin.config.ts",
    "build": "vite build",
    "build:admin": "vite build --config vite.admin.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:admin": "vite preview --config vite.admin.config.ts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "predeploy:admin": "npm run build:admin",
    "deploy:admin": "gh-pages -d dist-admin -r https://github.com/niuren111/UVAbackend.git -b gh-pages",
    "deploy-admin": "node deploy-admin.js",
    "deploy-simple": "node deploy-simple.js",
    "deploy-fixed": "node deploy-fixed-admin.js"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "antd": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.0.0",
    "gh-pages": "^6.3.0",
    "typescript": "^5.8.2",
    "vite": "^4.0.0"
  }
}
